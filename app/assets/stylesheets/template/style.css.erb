@font-face{
    font-family:'FontAwesome';
    src: url(<%= asset_path 'fontawesome-webfont.eot?v=4.0.1' %>);
    src: url(<%= asset_path 'fontawesome-webfont.eot?#iefix&v=4.0.1' %>) format('embedded-opentype'),
         url(<%= asset_path 'fontawesome-webfont.woff?v=4.0.1' %>) format('woff'),
         url(<%= asset_path 'fontawesome-webfont.ttf?v=4.0.1' %>) format('truetype'),
         url(<%= asset_path 'fontawesome-webfont.svg?v=4.0.1#fontawesomeregular' %>) format('svg');
    font-weight:normal;
    font-style:normal;
}

/*********************************************************************************/
/* Basic                                                                         */
/*********************************************************************************/
body
{
    background: #F8F8F8;
    margin: 0;
}

body,input,textarea,select
{
    font-family: 'Roboto', sans-serif;
    font-weight: 300;
    font-size: 13px;
    line-height: 1.25em;
}
#welcome {ont-family: 'Roboto', sans-serif;}
#welcome .dark h1,
#welcome .dark h2,
#welcome .dark h3,
#welcome .dark h4,
#welcome .dark h5,
#welcome .dark h6 {
    color: #fff;
}
#welcome .dark strong,
.dark b {
    color: #fff;
    color: rgba(255,255,255,0.85);
}
#welcome a {
    color: inherit;
    text-decoration: none;
    border-bottom: dotted 1px rgba(0,0,0,0.25);
    -webkit-transition: border-bottom-color 0.25s ease-in-out;
       -moz-transition: border-bottom-color 0.25s ease-in-out;
        -ms-transition: border-bottom-color 0.25s ease-in-out;
         -o-transition: border-bottom-color 0.25s ease-in-out;
            transition: border-bottom-color 0.25s ease-in-out;
}
#welcome a:hover{
    border-bottom-color: rgba(0,0,0,0);
}
#welcome .dark a{
    color: #fff;
    border-bottom-color: rgba(255,255,255,0.5);
}
#welcome .dark a:hover{
    text-decoration: none;
    border-bottom-color: rgba(255,255,255,0);
}
#welcome .dark hr{
    border-top-color: rgba(255,255,255,0.5);
}
#welcome blockquote{
    border-left: solid 0.25em #e6e6e6;
    padding: 1em 0 1em 2em;
    font-style: italic;
}
#welcome .dark blockquote{
    border-left-color: rgba(255,255,255,0.5);
}
#welcome p {
    text-align: justify;
}

#welcome header{
    margin-bottom: 1em;
}
#welcome header h1,
#welcome header h2,
#welcome header h3,
#welcome header h4,
#welcome header h5,
#welcome header h6{
    margin: 0;
}
#welcome header p{
    display: block;
    margin: 0;
    padding: 0.25em 0 0.5em 0;
}
#welcome footer{
    padding-top: 1.5em;
}
#welcome .featured{
    text-align: center;
}
#welcome .featured p{
    text-align: center;
}

/* Sections/Articles */
#welcome section,
#welcome article {
    margin-bottom: 3em;
}
#welcome section > :last-child,
#welcome article > :last-child{
    margin-bottom: 0;
}
#welcome section:last-child,
#welcome article:last-child		{
    margin-bottom: 0;
}
#welcome .row > section,
#welcome .row > article{
    margin-bottom: 0;
}

/* Images */
#welcome .image {
    position: relative;
    display: inline-block;
    border: 0;
}
#welcome .image:after{
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: url(<%= asset_path 'elements/overlay.png' %>);
}
#welcome .image img {
    display: block;
    width: 100%;
    border-radius: 0.5em;
}
#welcome .image.featured {
    display: block;
    width: 100%;
    margin: 0 0 2em 0;
}
#welcome .image.full {
    display: block;
    width: 100%;
}
#welcome .image.left {
    float: left;
    margin: 0 2em 2em 0;
}
#welcome .image.centered {
    display: block;
    margin: 0 0 2em 0;
}
#welcome .image.centered img {
    margin: 0 auto;
    width: auto;
}

/* Dark */
#welcome .dark {
    color: #aaa;
    color: rgba(255,255,255,0.65);
}
			

/*********************************************************************************/
/* Header                                                                        */
/*********************************************************************************/
#welcome #header {
    position: relative;
    margin: 0;
    background: url(<%= asset_path 'header.jpg' %>);
    background-size: cover;
    background-position: center center;
    padding: 150px 0 180px 0;
    text-align: center;
    color: #fff;
}

#welcome #header:after{
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: url(<%= asset_path 'elements/overlay.png' %>);
}

#welcome #header > header,
#welcome #header > header p,
#welcome #header > footer {
    text-align: center;
    position: relative;
    z-index: 1;
}
#welcome #header header h1 {
    font-size: 36px;
    line-height: 1.25em;
    margin-bottom: 0;
    font-family: 'Roboto', sans-serif;
}
#welcome #header header p {
    margin-top: 1.25em;
    font-weight: 100;
    padding: 0;
    font-size: 28px;
    line-height: 1.5em;
}
#welcome #header footer	{
    padding-top: 1.5em;
}
#welcome .btn-registration{
    font-size: 18px;
}
#welcome .dark .button {
    background: rgba(255,255,255,0.15);
    box-shadow: inset 0 0 0 1px #fff;
    color: #fff;
}
#welcome .button {
    -webkit-appearance: none;
    position: relative;
    display: inline-block;
    background: #3d3d3d;
    padding: 0.85em 3em 0.85em 3em;
    border-radius: 0.25em;
    cursor: pointer;
    border: 0;
    color: #fff;
    text-align: center;
    text-decoration: none;
    -moz-transition: all 0.25s ease-in-out;
    -webkit-transition: all 0.25s ease-in-out;
    -o-transition: all 0.25s ease-in-out;
    -ms-transition: all 0.25s ease-in-out;
    transition: all 0.25s ease-in-out;
}

/*********************************************************************************/
/* Main Sections                                                                 */
/*********************************************************************************/
#welcome .main {
    position: relative;
    margin: 0;
}
#welcome .main > header {
    background: #fff;
    text-align: center;
    padding: 5em 0 5em 0;
    margin: 0;
}
#welcome .main > header h2 {
    font-size: 28px;
    font-weight: 100;
    margin-bottom: 0;
}
#welcome .main > header p {
    margin: 2em 0 0 0;
    padding: 0;
    text-align: center;
    font-size: 18px;
    line-height: 1.5em;
}
#welcome .main > .content {
    padding: 6em 0 6em 0;
}
#welcome .main > .content h3{
    font-size: 1.5em;
}
#welcome .main > .content.dark{
    background: #433;
}
#welcome .main > .content.style1{
    background:	url(<%= asset_path 'elements/bgtr.svg' %>) top right no-repeat,
                url(<%= asset_path 'elements/bgbl.svg' %>) bottom left no-repeat,
                url(<%= asset_path 'elements/overlay.png' %>),
                linear-gradient(45deg, #b39c68, #a56365, #412e4c);
}
#welcome .main > .content.style2{
    background:	url(<%= asset_path 'elements/bgtr.svg' %>) top right no-repeat,
                url(<%= asset_path 'elements/bgbl.svg' %>) bottom left no-repeat,
                url(<%= asset_path 'elements/overlay.png' %>),
                linear-gradient(45deg, #384955, #655361, #85505f);
}
#welcome .main > .content.style3{
    background:	url(<%= asset_path 'elements/bgtr.svg' %>) top right no-repeat,
                url(<%= asset_path 'elements/bgbl.svg' %>) bottom left no-repeat,
                url(<%= asset_path 'elements/overlay.png' %>),
                linear-gradient(45deg, #5f796b, #3a4e59, #2f394e);
}
#welcome .main > .content.style4{
    padding-top: 0;
    background: #fff;
}